#include <stdio.h>
int main()
{
    char d[20];
    int i,j,a,b=0;
    printf("Enter some characters : ");
    gets(d);
    a=strlen(d);
    for(i=0; i<(a-b);i+=0)
    {
        if(d[i]==d[i+1])
        {
            for(j=i+1;j<=(a-b);j++)
                d[j]=d[j+1];
            b++;
        }
        else
            i++;
    }
    printf("Desired String : %s",d);
    return 0;
}
